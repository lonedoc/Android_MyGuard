<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#C5C5C7" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item android:drawable="@drawable/clear_icon"
        android:gravity="center"
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <scale android:scaleHeight="5dp" android:scaleWidth="5dp"/>
    </item>
</layer-list>
